{
    "name": "tabaoman/laravel-translation",
    "description": "An i18n solution with Laravel",
    "keywords": ["laravel", "translation", "multilanguage", "database", "i18n"],
    "require": {
        "php": "^7.2",
        "laravel/framework": "5.8.* || ^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "tony wang",
            "email": "wp0126@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tabaoman\\Translation\\": "src/Translation/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tabaoman\\Translation\\TranslationServiceProvider"
            ]
        }
    }
}
